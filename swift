print from sunday to monday USING SWITCH CASE  
var day="mon"
switch day{
    case "mon":
    print("monday")
    case "tue":
    print("tuesday")
    case "wed":
    print("wednesday")
    case "thurs":
    print("thursday")
    case "fri":
    print("friday")
    case "sat":
    print("saturday")
    case "sun":
    print("sunday")
    default:
    print("day doesn't exist")
}

    var n : Int
    var s : Int = 1
    n=5
    n=n-1
    while n != 0{
    s+=(s*n)
    n=n-1
}
    print(s)


